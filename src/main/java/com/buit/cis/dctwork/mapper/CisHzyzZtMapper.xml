		where JLXH in
        <foreach collection="jlxhList" item="z" open="(" close=")" separator=",">
            #{z}
        </foreach>
    </update>
	
	<!-- 已执行医嘱组套取消更新 -->
	<update id="updateBqyzZtExecuted">
