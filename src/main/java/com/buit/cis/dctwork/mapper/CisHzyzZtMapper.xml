   <update id="updateBqyzZtStopOrderReview">
	   UPDATE CIS_HZYZ_ZT
	   SET TZFHBZ = 1, TZFHR = #{fhgh}, TZFHSJ = #{fhsj}
	 WHERE JLXH = #{jlxh}
	   AND ((YSBZ = 1 and YSTJ = 1) or YSBZ = 0)
	   and (TZFHBZ = 0 or TZFHBZ is null)
	   AND JGID = #{jgid}
	   and (ZFBZ = 0 OR YZZF IS NULL)
   </update>
   
   <!-- 住院检验申请查询 -->
   <select id="queryZyJySqdList" resultType="com.buit.cis.dctwork.response.CisHzyzZtQueryResp">
       select a.ZXKS as ZXKS,
       b.BRCH as BRCH,
       b.BRXM as BRXM,
       b.ZYHM as ZYHM,
       a.ZYH as ZYH,
       a.KSSJ as KSSJ,
       a.YSGH as YSGH,
       a.YZMC as YZMC,
       a.ZFBZ as ZFBZ,
       (select RECEIPTDATE from api_lis_report where req_no = a.jlxh limit 1) as BGSJ
       from CIS_HZYZ_ZT a
       inner join IM_HZRY b
       on a.ZYH = b.ZYH
		 where 1 = 1
		 <if test="@sqlt.Ognl@isNotEmpty(yzlx)"> 
             and a.YZLX = #{yzlx}
         </if> 
		 <if test="@sqlt.Ognl@isNotEmpty(beginDate)">
             and date_format(a.KSSJ,'%Y-%m-%d') &gt;= #{beginDate}
    	 </if>
    	 <if test="@sqlt.Ognl@isNotEmpty(endDate)">
        	 and date_format(a.KSSJ,'%Y-%m-%d') &lt;= #{endDate}
    	 </if>
    	 <if test="@sqlt.Ognl@isNotEmpty(zxks)">
        	 and a.ZXKS = #{zxks}
    	 </if>
    	 <if test="@sqlt.Ognl@isNotEmpty(brbq)">
        	 and b.BRBQ = #{brbq}
    	 </if>  
    	 <if test="@sqlt.Ognl@isNotEmpty(zyh)">
        	 and a.ZYH = #{zyh}
    	 </if>  
    	 <if test="@sqlt.Ognl@isNotEmpty(stop)">
           	and a.ZFBZ = #{stop}
       	 </if>
		ORDER BY a.KSSJ desc
   </select>
   
