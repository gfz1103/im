        </if>
    </select>
    <!--     业务sql      -->
    
    <!--     更新组套医嘱提交状态      -->
    <update id="updateBqyzZtSubmitStatus" >
        update CIS_HZYZ_ZT
	   SET YSTJ = 1,
	   TJSJ = sysdate()
	 where YSGH = #{YSGH}
	   and YSTJ = 0
	   and YSBZ = 1
	   and LSYZ = #{LSYZ}
	   and ZYH = #{ZYH}
	   and JGID = #{JGID}
	   <if test="@sqlt.Ognl@isNotEmpty(ztJlxhList)"> 
           	 and JLXH not in 
	        <foreach collection="ztJlxhList" item="z" open="(" close=")" separator=",">
	            #{z}
	        </foreach>
       </if>  
    </update>
    
    <!--  根据记录序号取消修改医嘱组套提交状态     -->
    <update id="cancelZtSubmitStatusByJlxh" >
       update CIS_HZYZ_ZT set YSTJ = #{ystjnew}, BZXX = #{bzxx} 
       where YZZH in (select a.YZZH from (select yzzh from CIS_HZYZ_ZT
		where JLXH = #{jlxh}
		<if test="@sqlt.Ognl@isNotEmpty(ystj)">
           AND YSTJ = #{ystj}
       	</if>
		) a )
    </update>
    
